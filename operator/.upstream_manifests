                                  - deltaId
                                  - disk
                                  type: object
                                type: array
                              end:
                                format: date-time
                                type: string
                              removeTaskId:
                                type: string
                              snapshot:
                                type: string
                              start:
                                format: date-time
                                type: string
                            type: object
                          type: array
                        successes:
                          type: integer
                      required:
                      - consecutiveFailures
                      - failures
                      - successes
                      type: object
                  required:
                  - phase
                  - pipeline
                  type: object
                type: array
            type: object
        type: object
    served: true
    storage: true
