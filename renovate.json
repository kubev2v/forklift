{
    "packageRules": [
      {
        "matchBaseBranches": ["/^release.*/"],
        "matchManagers": ["dockerfile", "rpm", "gomod"],
        "enabled": false
      }
    ],
    "autoApprove": true,
    "tekton": {
        "automerge": true,
        "automergeStrategy": "rebase",
        "automergeType": "pr",
        "enabled": true,
        "platformAutomerge": true
    }
}