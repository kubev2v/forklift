kind: CustomResourceDefinition
metadata:
  annotations:
    controller-gen.kubebuilder.io/version: v0.17.3
  name: networkmaps.forklift.konveyor.io
spec:
  group: forklift.konveyor.io
  names:
    kind: NetworkMap
    listKind: NetworkMapList
    plural: networkmaps
    singular: networkmap
